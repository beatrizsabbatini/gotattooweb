'@GoTattoo:token');\n    const user = localStorage.getItem('@GoTattoo:user');\n\n    if (token && user) {\n      return { token, user: JSON.parse(user) };\n    }\n\n    return {} as AuthState;\n  });\n\n  const signIn = useCallback(async ({ email, password }) => {\n    const response = await api.post('sessions', {\n      email,\n      password,\n    });\n\n    const { token, user } = response.data;\n\n    console.log(user);\n\n    localStorage.setItem('@GoTattoo:token', token);\n    localStorage.setItem('@GoTattoo:user', JSON.stringify(user));\n\n    setData({ token, user });\n  }, []);\n\n  const signOut = useCallback(() => {\n    localStorage.removeItem('@GoTattoo:token');\n    localStorage.removeItem('@GoTattoo:user');\n\n    setData({} as AuthState);\n  }, []);\n\n  return (\n    <AuthContext.Provider value={{ user: data.user, signIn, signOut }}>\n      {children}\n    </AuthContext.Provider>\n  );\n};\n\nfunction useAuth(): AuthContextData {\n  const context = useContext(AuthContext);\n\n  if (!context) {\n    throw new Error('UseAuth must be used within an AuthProvider');\n  }\n\n  return context;\n}\n\nexport { AuthProvider, useAuth };\n","/home/beatriz/Área de Trabalho/Bootcamp GoStack/gotattoo-web/src/hooks/Toast.tsx",[],"/home/beatriz/Área de Trabalho/Bootcamp GoStack/gotattoo-web/src/hooks/index.tsx",[],"/home/beatriz/Área de Trabalho/Bootcamp GoStack/gotattoo-web/src/components/ToastContainer/Toast/index.tsx",[],"/home/beatriz/Área de Trabalho/Bootcamp GoStack/gotattoo-web/src/components/ToastContainer/Toast/styles.ts",[],"/home/beatriz/Área de Trabalho/Bootcamp GoStack/gotattoo-web/src/routes/index.tsx",[],"/home/beatriz/Área de Trabalho/Bootcamp GoStack/gotattoo-web/src/pages/Dashboard/index.tsx",[],"/home/beatriz/Área de Trabalho/Bootcamp GoStack/gotattoo-web/src/routes/Route.tsx",[],{"ruleId":"111","replacedBy":"112"},{"ruleId":"113","replacedBy":"114"},{"ruleId":"115","replacedBy":"116"},{"ruleId":"117","replacedBy":"118"},{"ruleId":"119","replacedB